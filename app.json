{
  "expo": {
    "name": "proxalert",
    "slug": "proxalert",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyAJwBJZ7YHp0T_oEAyHjsseeCmQI5uQOj0"
      },
      "bundleIdentifier": "com.proxalert"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAJwBJZ7YHp0T_oEAyHjsseeCmQI5uQOj0"
        }
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.proxalert"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "83c88c18-f9b5-4b4f-8ca1-1fc39452f124"
      }
    }
  }
}